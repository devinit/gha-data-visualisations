{
  "name": "di-chart-boilerplate",
  "version": "1.0.0",
  "description": "A Starting Point for Creating DI's D3 Charts",
  "private": true,
  "scripts": {
    "build": "nwb build-web-app && gulp",
    "clean": "nwb clean-app",
    "start": "nwb serve-web-app --reload",
    "test": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server",
    "lint": "eslint 'src/**/*.js' --quiet"
  },
  "devDependencies": {
    "@types/d3": "^4.13.12",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.4",
    "nwb": "0.25.x"
  },
  "author": "",
  "license": "MIT",
  "repository": "",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "core-js": "^3.15.2",
    "deepmerge": "^4.2.2"
  }
}
